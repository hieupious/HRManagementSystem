@{ 
    ViewData["Title"] = "Tram Ngoc Minh - Dashboard";
}
<div class="row">
    <div class="m-t-lg m-b-lg clearfix">
        <div class="pull-left">
            <h2 class="m-a-0 text-uppercase">@ViewData["Title"] <small class="text-muted">ASWVN431 - GPTower</small></h2>
        </div>
    </div>
    <div class="col-lg-12 panel panel-primary">
        <div class="panel-body">
            <ul class="list-group">
                <li class="list-group-item"><strong>Department: </strong>Developer</li>
                <li class="list-group-item"><strong>Working Hour Rule Applied: </strong>Devs/QAs</li>
                <li class="list-group-item"><strong>Check-in Time Today: </strong>8:00 A.M</li>
                <li class="list-group-item list-group-item-danger"><strong>Total Lacking Time In Month: </strong>45 minutes</li>
            </ul>
            <div class="col-lg-6">
                @Html.ActionLink("View Timesheet", "UserInfo", "Home", null, new { @class = "btn btn-primary pull-left" })
            </div>
            <div class="col-lg-6">
                <div class="pull-right danger" data-toggle="tooltip" data-placement="left" title="Request admin change informations if incorrectt">
                <u>Request For Change</u>
             </div>
            </div>
        </div>
       
     </div>
    
 </div>
